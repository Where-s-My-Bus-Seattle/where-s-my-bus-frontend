{
  "expo": {
    "name": "Where's My Bus? (Seattle)",
    "slug": "wheres_my_bus",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "description": "Application designed for commuters - You know what bus you want (it is the same one each day) and you also know where it goes. Find out quickly when your bus gets 'here'!",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/button.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
